hostname uClinux
/bin/expand /etc/ramfs.img /dev/ram1
mount -t proc proc /proc
mount -t ext2 /dev/ram1 /var
mkdir /var/tmp
mkdir /var/log
mkdir /var/run
mkdir /var/lock
mkdir /var/empty
mkdir /var/flash
mkdir /var/systeme
/home/pmvWatchdog -force -verbose 
/home/pmvWatchdog  &
mount -t jffs2 /dev/mtdblock3 /var/flash
#mount -t jffs2 /dev/mtdblock2 /var/systeme
ifconfig lo 127.0.0.1
route add -net 127.0.0.0 netmask 255.0.0.0 lo
chmod 777 /var/flash
chmod 777 /var/systeme
cd /var/flash
/home/pmvLanceur -prog pmvMain
cat /etc/motd
/var/flash/pmvMain &
